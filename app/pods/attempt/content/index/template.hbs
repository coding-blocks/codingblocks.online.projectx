{{!-- Render a component based upon payload --}}
{{#if payload}}
  {{component componentName payload=payload onVideoCompleted=(action 'transitonToNextContent') tab=tab run=run start=start showFeedback=(unbound showFeedback) onFeedbackSubmit=(action 'submitFeedback')}}
{{else}}
  {{locked-content course=run.course}}
{{/if}}